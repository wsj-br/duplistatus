{
  "name": "duplistatus",
  "version": "0.8.4",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.15.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "./scripts/update-version.sh && next build",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production HOSTNAME=0.0.0.0 PORT=9666 node duplistatus-server.ts",
    "cron:start": "tsx src/cron-service/index.ts",
    "dev": "./scripts/update-version.sh && next dev -p 8666",
    "cron:dev": "CRON_PORT=8667 tsx watch src/cron-service/index.ts",
    "build-local": "./scripts/update-version.sh && next build",
    "start-local": "./scripts/update-version.sh && NODE_ENV=production PORT=8666 CRON_PORT=8667 NEXT_TELEMETRY_DISABLED=1 tsx duplistatus-server.ts",
    "cron:start-local": "NODE_ENV=production CRON_PORT=8667 tsx watch src/cron-service/index.ts",
    "generate-test-data": "tsx scripts/generate-test-data.ts",
    "show-overdue-notifications": "tsx scripts/show-overdue-notifications.ts",
    "run-overdue-check": "tsx scripts/run-overdue-check.ts",
    "test-cron-port": "tsx scripts/test-cron-port.ts",
    "release:patch": "pnpm version patch",
    "release:minor": "pnpm version minor",
    "release:major": "pnpm version major",
    "postversion": "git push && git push --tags",
    "docker-up": "docker compose up --build -d",
    "docker-down": "docker compose down",
    "docker-clean": "./scripts/clean-docker.sh"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/qrcode": "^1.5.5",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "node-cron": "^4.2.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "recharts": "^3.2.1",
    "string-template": "^1.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/react": "19.1.13",
    "@types/string-template": "^1.0.7",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "webpack": "^5.101.3"
  }
}
