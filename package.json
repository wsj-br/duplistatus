{
  "name": "duplistatus",
  "version": "1.3.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=24.12.0",
    "pnpm": ">=10.24.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "./scripts/pre-checks.sh && pnpm intlayer build && next build --webpack",
    "lint": "pnpm exec eslint .",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production HOSTNAME=0.0.0.0 PORT=9666 node .next/standalone/server.js",
    "cron:start": "tsx src/cron-service/index.ts",
    "dev": "./scripts/pre-checks.sh && pnpm intlayer build && NODE_ENV=development PORT=8666 next dev",
    "cron:dev": "CRON_PORT=8667 tsx watch src/cron-service/index.ts",
    "build-local": "./scripts/pre-checks.sh && pnpm intlayer build && next build && mkdir -p .next/standalone/.next && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "start-local": "./scripts/pre-checks.sh && NODE_ENV=production HOSTNAME=0.0.0.0 PORT=8666 CRON_PORT=8667 NEXT_TELEMETRY_DISABLED=1 node .next/standalone/server.js",
    "cron:start-local": "NODE_ENV=production CRON_PORT=8667 tsx watch src/cron-service/index.ts",
    "generate-test-data": "tsx scripts/generate-test-data.ts",
    "test-overdue-detection": "tsx scripts/test-overdue-detection.ts",
    "validate-csv-export": "tsx scripts/validate-csv-export.ts",
    "show-overdue-notifications": "tsx scripts/show-overdue-notifications.ts",
    "run-overdue-check": "tsx scripts/run-overdue-check.ts",
    "test-cron-port": "tsx scripts/test-cron-port.ts",
    "take-screenshots": "sh -c 'tsx --env-file=.env scripts/take-screenshots.ts \"$@\" 2>&1 | tee take-screenshots.log' _",
    "set-smtp-test-config": "tsx --env-file=.env scripts/set-smtp-test-config.ts",
    "test-smtp-connections": "tsx --env-file=.env scripts/test-smtp-connections.ts",
    "docker-up": "./scripts/pre-checks.sh && docker compose up --build -d",
    "docker-down": "docker compose down",
    "docker-clean": "./scripts/clean-docker.sh",
    "docker-devel": "docker build . -t wsj-br/duplistatus:devel",
    "test-entrypoint": "./scripts/test-entrypoint.sh"
  },
  "dependencies": {
    "@intlayer/swc": "^8.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcrypt": "^6.0.0",
    "@types/nodemailer": "^7.0.9",
    "@types/qrcode": "^1.5.6",
    "autoprefixer": "^10.4.24",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "intlayer": "^8.0.2",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-intlayer": "^8.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^8.0.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-datepicker": "^9.1.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-intlayer": "^8.0.2",
    "recharts": "^3.7.0",
    "string-template": "^1.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "19.2.11",
    "@types/string-template": "^1.0.7",
    "baseline-browser-mapping": "latest",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "puppeteer": "^24.36.1",
    "tsx": "^4.21.0",
    "webpack": "^5.105.0"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1",
      "preact": ">=10.28.2",
      "lodash": ">=4.17.23",
      "lodash-es": ">=4.17.23",
      "@isaacs/brace-expansion": ">=5.0.1"
    }
  }
}